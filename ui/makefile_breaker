# makefile para breaker, versi√≥n 1 
# uso de variables; reglas predefinidas

CC = gcc -pthread
OBJECTS = devices/reduction.o lib/spclib.o lib/hashTable3.o lib/charset.o lib/space.o tools/breaker.o

break : ui/main_breaker.o $(OBJECTS)
	$(CC) -o break ui/main_breaker.o $(OBJECTS) -lm -lssl -lcrypto

ui/main_breaker.o: tools/breaker.h
lib/hashTable3.o: lib/hashTable3.h
lib/charset.o: lib/charset.h
lib/space.o: lib/space.h lib/charset.h
lib/spclib.o: lib/spclib.h
devices/reduction.o: devices/reduction.h lib/space.h lib/space.h
tools/breaker.o: tools/breaker.h devices/reduction.h lib/spclib.h lib/hashTable3.h



.PHONY : clean
    clean :
	-rm break $(objects) 

# makefile para breaker, versi√≥n 1 
# uso de variables; reglas predefinidas

CC = gcc -pthread
OBJECTS = ../devices/reduction.o ../lib/util.o ../lib/hashTable3.o ../lib/keyspace.o ../tools/break.o

rbt_break : breaker_ui.o $(OBJECTS)
	$(CC) -o rbt_break breaker_ui.o $(OBJECTS) -lm -lssl -lcrypto

breaker_ui.o: ../tools/break.h
../lib/hashTable3.o: ../lib/hashTable3.h
../lib/keyspace.o: ../lib/keyspace.h
../lib/util.o: ../lib/util.h
../devices/reduction.o: ../devices/reduction.h ../lib/keyspace.h
../tools/break.o: ../tools/break.h ../devices/reduction.h ../lib/util.h ../lib/hashTable3.h



.PHONY : clean
    clean :
	-rm rbt_break $(OBJECTS) 
